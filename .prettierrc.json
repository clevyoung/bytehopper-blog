{
  "$schema": "https://json.schemastore.org/prettierrc.json",
  "tabWidth": 2,
  "printWidth": 100,
  "semi": true,
  "useTabs": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "endOfLine": "auto",
  "arrowParens": "always",
  "trailingComma": "es5",
  "plugins": ["prettier-plugin-tailwindcss"]
}
